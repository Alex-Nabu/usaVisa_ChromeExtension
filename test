function delayedFunction() {
  // Code to be executed after 5 seconds
  console.log("Delayed function executed");

  setTimeout(delayedFunction2, 5000);
}

function delayedFunction() {
  // Code to be executed after 5 seconds
  console.log("Delayed function executed");

  setTimeout(previousFunction, 5000);
}

function previousFunction() {
  // Code of the previous function
  console.log("Previous function executed");

  // Delay execution of the delayedFunction by 5 seconds
  setTimeout(delayedFunction, 5000);
}

// Call the previous function
previousFunction();